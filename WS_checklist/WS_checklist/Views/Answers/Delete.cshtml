@model WS_checklist.Models.Answer

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>Answer</legend>

    <div class="display-label">Rating</div>
    <div class="display-field">@Model.Rating</div>

    <div class="display-label">Comment</div>
    <div class="display-field">@Model.Comment</div>

    <div class="display-label">Question</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.Question).ToString()</div>

    <div class="display-label">SiteVisit</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.SiteVisit).ToString()</div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}


